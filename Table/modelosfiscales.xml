<?xml version="1.0" encoding="UTF-8"?>
<table>
    <column>
        <name>idmodelo</name>
        <type>serial</type>
    </column>
    <column>
        <name>tipo</name>
        <type>character varying(10)</type>
    </column>
    <column>
        <name>periodo</name>
        <type>character varying(10)</type>
    </column>
    <column>
        <name>codejercicio</name>
        <type>character varying(4)</type>
    </column>
    <column>
        <name>fechainicio</name>
        <type>date</type>
    </column>
    <column>
        <name>fechafin</name>
        <type>date</type>
    </column>
    <column>
        <name>fechapresentacion</name>
        <type>date</type>
        <null>YES</null>
    </column>
    <column>
        <name>idregiva</name>
        <type>integer</type>
        <null>YES</null>
    </column>
    <column>
        <name>idrectifica</name>
        <type>integer</type>
        <null>YES</null>
    </column>
    <column>
        <name>totaldevengado</name>
        <type>double precision</type>
        <default>0</default>
    </column>
    <column>
        <name>totaldeducible</name>
        <type>double precision</type>
        <default>0</default>
    </column>
    <column>
        <name>resultado</name>
        <type>double precision</type>
        <default>0</default>
    </column>
    <column>
        <name>estado</name>
        <type>character varying(20)</type>
        <default>'borrador'</default>
    </column>
    <column>
        <name>numeroreferencia</name>
        <type>character varying(50)</type>
        <null>YES</null>
    </column>
    <column>
        <name>fechacreacion</name>
        <type>timestamp</type>
    </column>
    <constraint>
        <name>modelosfiscales_pkey</name>
        <type>PRIMARY KEY (idmodelo)</type>
    </constraint>
    <constraint>
        <name>ca_modelosfiscales_ejercicio</name>
        <type>FOREIGN KEY (codejercicio) REFERENCES ejercicios (codejercicio) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_modelosfiscales_rectifica</name>
        <type>FOREIGN KEY (idrectifica) REFERENCES modelosfiscales (idmodelo) ON DELETE SET NULL ON UPDATE CASCADE</type>
    </constraint>
</table>
